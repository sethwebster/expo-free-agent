{
  "name": "expo-free-agent",
  "version": "0.1.23",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "controller": "cd packages/controller-elixir && mix phx.server",
    "controller:dev": "cd packages/controller-elixir && mix phx.server",
    "landing-page:dev": "bun --cwd packages/landing-page dev",
    "landing-page:build": "bun --cwd packages/landing-page build",
    "landing-page:preview": "bun --cwd packages/landing-page preview",
    "landing-page:deploy": "bun --cwd packages/landing-page deploy",
    "test": "bun test",
    "test:controller": "cd packages/controller-elixir && mix test",
    "test:cli": "bun test packages/cli/src/__tests__",
    "test:e2e": "./test-e2e.sh",
    "test:versions": "bun run scripts/check-versions.ts",
    "test:docs": "./scripts/verify-docs.sh",
    "test:all": "bun test && ./test-e2e.sh && ./scripts/verify-docs.sh"
  },
  "devDependencies": {
    "archiver": "^7.0.1"
  }
}
