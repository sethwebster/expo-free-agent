{
  "name": "@sethwebster/expo-free-agent",
  "version": "0.1.16",
  "description": "CLI for Expo Free Agent distributed build system",
  "type": "module",
  "bin": {
    "expo-free-agent": "./dist/index.js"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "bun build src/index.ts --outdir=dist --target=node",
    "dev": "bun run src/index.ts",
    "typecheck": "tsc --noEmit",
    "test": "bun test src/__tests__",
    "prepublishOnly": "bun run build"
  },
  "keywords": [
    "expo",
    "build",
    "distributed",
    "ios",
    "react-native"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/expo/expo-free-agent.git",
    "directory": "cli"
  },
  "dependencies": {
    "archiver": "^7.0.1",
    "chalk": "^5.4.1",
    "cli-progress": "^3.12.0",
    "commander": "^12.1.0",
    "form-data": "^4.0.1",
    "ora": "^8.1.1",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@types/archiver": "^6.0.3",
    "@types/cli-progress": "^3.11.6",
    "@types/node": "^22.10.5",
    "typescript": "^5.7.3"
  },
  "engines": {
    "node": ">=18"
  }
}
